<!DOCTYPE html>
<html>
  <head>
    <title><%= t"elearning" %></title>
    <%= stylesheet_link_tag "application", media: "all",
      "data-turbolinks-track" => true %>
    <%= javascript_include_tag "application",
      "data-turbolinks-track" => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <%= render t("layoutheader") %>
    <div id="flash-msg">
      <%= render "shared/flash_message" %>
    </div>
    <div class="container">
      <%= yield %>
      <%= render t("layoutfooter") %>
    </div>
  </body>
</html>

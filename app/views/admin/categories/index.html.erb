<div class="container">
<% provide(:title, t("all_categories")) %>
<h1><%= t"All_categories" %></h1>
<div class="row text-left" style="padding: 20px">
  <%=link_to t("create"), new_admin_category_path,
    class: "btn btn-lg btn-primary"%>
</div>
<%= will_paginate %>
<ul class="users">
  <table class="table table-hover">
    <thead>
      <tr>
        <th><%= t"id" %></th>
        <th><%= t"category" %></th>
        <th><%= t("action") %></th>
      </tr>
    </thead>
    <tbody>
      <%= render "category" %>
    </tbody>
  </table>
</ul>
</div>
